
.New_{
  .content {
    width: 1200px
    margin 0 auto
    margin-bottom 40px
    position relative
    .crumbs{
      font-size: 14px;
      color: #999999;
      line-height: 26px;
      margin 10px 0
    }
    .New_wrap{
      display flex
      .nav_left{
        width: 180px
        margin-right 35px
        .wrap_left{
          position relative
          top:0
          left:0
          background: #fff
          position sticky
          top:0
          left:0
          &.active{
            position sticky
            top:0
            left:0
          }
          .navHeader{
            width: 180px;
            height: 65px;
            background-color: rgba(0, 92, 161, 1);
            display flex
            align-items center
            justify-content center
            >img{
              width: 25px
              height: 30px
              margin-right 10px
            }
            .card_{
              >p{
                font-weight: 700;
                font-style: normal;
                color: #FFFFFF;
                font-size 18px
              }
              >span{
                font-size: 14px;
                color: #FFFFFF;
              }
            }
          }
          .navList{
            .item_nav{
              margin-top 10px
              height: 50px
              width: 180px
              background-color: rgba(242, 242, 242, 1);
              display flex
              align-items center
              justify-content center
              cursor: pointer
              font-weight: 700;
              font-size: 16px;
              color: #666666;
              position relative
              &:before{
                content: ''
                display none
                width: 10px
                height: 100%
                background: #005CA1
                position absolute
                top: 0
                left: 0
              }
              &.active{
                color: #005CA1
                &:before{
                  display block
                }
              }
            }
          }

        }

      }
      .content_right{
        flex 1
        .cjh_news{
          .navList{
            display flex
            justify-content space-between
            .item{
              width: 180px
              height: 50px
              background: #f2f2f2
              line-height: 50px
              text-align: center
              font-size: 16px
              color: #666
              font-weight: 700
              position relative
              display flex
              flex-direction column
              align-items center
              cursor: pointer
              &:after{
                opacity 0
                content ''
                width: 0;
                height: 0;
                border-style: solid;
                border-width: 20px 16px 0 16px;
                border-color:  #f2f2f2 transparent transparent transparent;
              }
              &.active{
                color: #fff
                background: #005ca1
                &:after{
                  opacity 1
                  border-color:  #005ca1 transparent transparent transparent;
                }
              }
            }
          }
        }

        .content_right{
          .news_header{
            display flex
            >img{
              width: 388px;
              height: 333px;
              flex-shrink 1
              margin-right 20px
            }
            .header_text{
              flex:1
              .title{
                font-weight: 700;
                font-style: normal;
                font-size: 22px;
                color: #005CA1;
                max-width 570px
                line-height: 24px;
                overflow: hidden;
                text-overflow: ellipsis;
                white-space: nowrap;
                cursor: pointer
              }
              >p{
                margin-top 20px
                color: #999999;
                font-size: 14px;
                line-height: 24px;
                text-overflow: -o-ellipsis-lastline;
                overflow: hidden;
                text-overflow: ellipsis;
                display: -webkit-box;
                -webkit-line-clamp: 6;
                -webkit-box-orient: vertical;
                cursor: pointer

              }
              .list{
                margin-top 20px
                .item_text{
                  display flex
                  justify-content space-between
                  align-items center
                  padding-bottom 10px
                  border-bottom 1px dashed #999999
                  margin-bottom 20px
                  &:last-child{
                  }
                  cursor: pointer
                  >div{
                    font-size: 16px;
                    line-height: 24px;
                    color: #333
                    max-width 450px
                    overflow: hidden;
                    text-overflow: ellipsis;
                    white-space: nowrap;
                  }
                  >p{
                    font-size: 14px;
                    line-height: 24px;
                    color: #333
                  }
                }
              }
            }
          }
          .news_pagination{
            margin-top 30px
            .text_list{
              .item_text{
                display flex
                justify-content space-between
                align-items center
                padding-bottom 10px
                border-bottom 1px dashed #999999
                margin-bottom 20px
                &:last-child{
                }
                cursor: pointer
                >div{
                  font-size: 16px;
                  line-height: 24px;
                  color: #333
                  max-width 700px
                  overflow: hidden;
                  text-overflow: ellipsis;
                  white-space: nowrap;
                }
                >p{
                  font-size: 14px;
                  line-height: 24px;
                  color: #333
                }
              }
            }
          }
        }
      }
    }
  }
}
