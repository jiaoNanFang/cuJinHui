    .branch_sw{
      width: 1000px
      height: 320px
      margin-top 20px
      position: relative;
      margin 0 auto
      .swiper-container{
        width: 1000px
        .swiper-wrapper{
          .swiper-slide{
            display flex
            justify-content space-between
            .swiper_listBox{
              width: 285px
              height: 320px
              display flex
              flex-direction column
              align-items center
              //justify-content space-around
              .slide_top{
                width: 285px
                height: 162px
