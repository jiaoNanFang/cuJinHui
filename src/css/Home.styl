            }
          }
          >>> .swiper-button-prev{
            transform translateY(-100px)
          }
        }
      }
      .swiper-button-prev {
        //background: #fafafa url("../assets/images/home/u311.png") no-repeat center center;
        left:-100px
      }

      .swiper-button-next {
        //background: #fafafa url("../assets/images/home/u312.png") no-repeat center center;
        right:-100px
      }
      .swiper-container {
        width:calc(100% - 30px);
        margin:auto;
      }

    }


