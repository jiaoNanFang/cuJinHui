      margin-top 20px
      .swiper-container{
        .swiper-wrapper{
          .swiper-slide{
            display flex
            justify-content space-between
            .swiper_listBox{
              width: 285px
              height: 320px
              display flex
              flex-direction column
              align-items center
              //justify-content space-around
              .slide_top{
                width: 285px
                height: 162px
                background #f2f2f2
                display flex
                align-items center
                justify-content center
                >img{
                  width: 285px
